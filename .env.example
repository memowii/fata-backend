# Application
NODE_ENV=development
PORT=5000

# Database - PostgreSQL
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
POSTGRES_USER=fata_user
POSTGRES_PASSWORD=fata_password
POSTGRES_DB=fata_db
DATABASE_URL=postgresql://fata_user:fata_password@postgres:5432/fata_db

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=fata_redis_password
REDIS_URL=redis://:fata_redis_password@redis:6379

# JWT (if using authentication)
# JWT_SECRET=your-secret-key
# JWT_EXPIRATION=3600

# External APIs
# API_KEY=your-api-key

# Logging
# LOG_LEVEL=debug

# CORS (if needed)
# CORS_ORIGIN=http://localhost:3000